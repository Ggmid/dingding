.date-radio {
  padding: 26rpx;
}

.date-radio label + label {
  margin-left: 20rpx;
}

.red{
  color: #f00;
}
.row {
  display: flex;
  align-items: center;
  padding: 0 30rpx;
}

.row-title {
  flex: 1;
  padding-top: 28rpx;
  padding-bottom: 28rpx;
  /* font-size: 34rpx; */
  color: #000;
}

.row-extra {
  flex-basis: initial;
  /* font-size: 32rpx; */
  color: #888;
}

.row-arrow {
  width: 32rpx;
  height: 32rpx;
  margin-left: 16rpx;
}
.row-border{
  border-bottom:1px solid #eee;
}