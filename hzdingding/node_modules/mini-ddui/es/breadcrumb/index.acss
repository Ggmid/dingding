.dd-breadcrumb {
  height: 48px;
  overflow: hidden;
}

.dd-breadcrumb-content {
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: flex-start;
  width: 100%;
  height: 56px;
  background-color: #fff;
  box-sizing: border-box;
}

.dd-breadcrumb-item {
  padding: 0 12px;
  color: #3296FA;
  font-size: 15px;
  line-height: 48px;
  white-space: nowrap;
}

.dd-breadcrumb-item:first-child {
  padding-left: 16px;
}

.dd-breadcrumb-item:last-child {
  padding-right: 16px;
}

.dd-breadcrumb-item-selected {
  color: rgba(25, 31, 37, .56);
}

.dd-breadcrumb-divider {
  padding-left: 12px;
  width: 0;
  height: 48px;
  background-image: url('data:image/png;base64,');
  background-size: contain;
  background-position: center;
  background-repeat: no-repeat;
  box-sizing: border-box;
}
