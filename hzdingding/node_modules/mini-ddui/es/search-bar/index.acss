.am-search {
  height: 52px;
  display: flex;
  align-items: center;
  padding: 0 0 0 12px;
  background: #ffffff;
}

.am-search-input {
  display: flex;
  position: relative;
  height: 36px;
  line-height: 36px;
  flex: 1;
  background: rgba(0, 0, 0, 0.05);
  border-radius: 3px;
}

.am-search-synthetic {
  display: flex;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
  height: 36px;
  line-height: 36px;
  padding-left: 12px;
}

.am-search-synthetic-placeholder {
  width: 100px;
  font-size: 15px;
  color: rgba(25, 31, 37, .4);
}

.am-search-synthetic-icon {
  height: 14px;
  width: 14px;
  display: inline-block;
  background-image: url('data:image/svg+xml;base64,');
  background-size: contain;
  margin-right: 8px;
  vertical-align: -2px;
}

.am-search-value {
  flex: 1;
  height: 36px;
  font-size: 15px;
  padding: 0 0 0 36px;
  background: transparent;
  box-sizing: border-box;
}

.am-search-clear {
  visibility: hidden;
  width: 36px;
  height: 36px;
}

.am-search-clear icon {
  display: flex;
  height: 100%;
  justify-content: center;
  align-items: center;
}

.am-search-clear-show {
  visibility: visible;
}

.am-search-cancel {
  margin-right: -48px;
  opacity: 0;
  color: #3296FA;
  font-size: 17px;
  /* width: 64px; */
  padding: 0 16px;
  height: 36px;
  line-height: 36px;
  text-align: center;
}

.am-search-anim {
  transition: margin-right 0.3s, opacity 0.3s;
}

.am-search-repos {
  margin-right: 0;
  opacity: 1;
}

.am-search-back {
  height: 24px;
  width: 24px;
  background-image: url('data:image/png;base64,');
  background-size: contain;
  background-repeat: no-repeat;
  margin-left: -40px;
  margin-right: 16px;
  transition: margin-left 0.3s, opacity 0.3s;
}

.am-search-back-end {
  margin-left: -8px;
}